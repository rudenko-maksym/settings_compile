{
    "name": "winmillwill/settings_compile",
    "authors": [
        {
            "name": "Will Milton",
            "email": "wa.milton@gmail.com"
        }
    ],
    "require": {
        "symfony/config": "*",
        "symfony/yaml": "*",
        "symfony/dependency-injection": "~2.6"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "extra": {
      "branch-alias": {
         "dev-master": "2.1.x-dev"
      }
    },
    "autoload": {
        "psr-4": {
            "Drupal\\Settings\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "bin": ["bin/settings_compile"],
    "config": {
        "bin-dir": "vbin"
    }
}
